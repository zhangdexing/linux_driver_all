
on post-fs-data

service  lidbg_load /system/bin/lidbg_load
    class core
    user root
    group root 
    oneshot

service vold /system/lib/modules/out/vold
    class core
    socket vold stream 0660 root mount
    ioprio be 2

service bootanim /system/lib/modules/out/bootanimation
    class core
    user root
    group graphics audio
    disabled
    oneshot
